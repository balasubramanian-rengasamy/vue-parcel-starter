{
  "name": "vue-parcel-starter",
  "version": "1.0.0",
  "description": "a simple vue.js starter with parcel",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "format": "prettier --write \"src/**/*.{vue,js}\"",
    "build": "parcel build ./src/index.html --public-url .",
    "build:ci": "parcel build ./src/index.html --public-url . --log-level 2",
    "test": "jest --config config/tests/jest.conf.js --coverage",
    "test:watch": "jest --config config/tests/jest.conf.js --coverage --watch"
  },
  "keywords": [
    "vue.js",
    "parcel",
    "vue",
    "parcel-bundler"
  ],
  "author": {
    "name": "Eric McCormick",
    "url": "https://edm00se.codes"
  },
  "repository": "edm00se/vue-parcel-starter",
  "homepage": "https://github.com/edm00se/vue-parcel-starter",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@vue/component-compiler-utils": "2.3.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "1.7.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "4.9.4",
    "parcel-bundler": "1.10.3",
    "prettier": "1.14.3",
    "rimraf": "^2.6.2",
    "vue-hot-reload-api": "2.3.1",
    "vue-jest": "3.0.0",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  }
}
