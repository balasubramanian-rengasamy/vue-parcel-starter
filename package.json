{
  "name": "vue-parcel-starter",
  "version": "1.0.0",
  "description": "a simple vue.js starter with parcel",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "format": "prettier --write \"src/**/*.{vue,js}\"",
    "build": "parcel build ./src/index.html --public-url .",
    "build:ci": "parcel build ./src/index.html --public-url . --log-level 2",
    "test": "jest --config config/tests/jest.conf.js --coverage",
    "test:watch": "jest --config config/tests/jest.conf.js --coverage --watch"
  },
  "keywords": [
    "vue.js",
    "parcel",
    "vue",
    "parcel-bundler"
  ],
  "author": {
    "name": "Eric McCormick",
    "url": "https://edm00se.codes"
  },
  "repository": "edm00se/vue-parcel-starter",
  "homepage": "https://github.com/edm00se/vue-parcel-starter",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^8.1.4",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "sass": "^1.17.2",
    "vue-hot-reload-api": "^2.3.3",
    "vue-jest": "^3.0.2",
    "vue-server-renderer": "^2.6.7",
    "vue-template-compiler": "^2.6.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": ""
    }
  },
  "lint-staged": {
    "*.{js,json,vue}": [
      "prettier --write",
      "git add"
    ]
  }
}
